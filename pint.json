{
    "preset": "laravel",
    "rules": {
        "method_argument_space": {
            "on_multiline": "ensure_fully_multiline"
        },
        "no_unused_imports": true,
        "ordered_imports": {
            "sort_algorithm": "alpha"
        }
    },
    "exclude": [
        "vendor",
        "storage",
        "bootstrap/cache",
        "node_modules",
        "public/dashboardAssets"
    ]
}